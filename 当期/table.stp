%JH='Project Sheet1 * * *',
%TCI='PAGE <&PAG><JR><J+>*TABLE <&TABLEIDENTIFICATION><JL><J+>',
%PTR='总体',
 %AVG='Mean<d2>', %MED='Median',  GLOBAL BT#='All Respondents',
